package com.dhas;

public class Main {
    public static void main(String args[]) {
       int arr[]={1,2,3,4,5,6,7,89,12};
       find(arr,89);
    }
    public static void find(int arr[],int target){
        for(int i=0;i<arr.length;i++){
            if(arr[i]==target){
                System.out.println(i);
                return;
            }
        }
    }
}
